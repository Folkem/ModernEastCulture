<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="settingsForm" prefHeight="450.0" prefWidth="800.0" stylesheets="@/img/style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.folva.moderneastculture.controller.SettingsController">
    <Button id="settings_exitButton" cancelButton="true" contentDisplay="TEXT_ONLY" layoutX="681.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBExitClick" text="%exitButton" textFill="WHITE" wrapText="true">
        <font>
            <Font name="Courier New Bold" size="24.0" />
        </font>
    </Button>
    <PasswordField fx:id="tbNewPassword" layoutX="105.0" layoutY="188.0" prefHeight="51.0" prefWidth="305.0" promptText="%newPassword">
        <font>
            <Font name="Century Gothic Italic" size="24.0" />
        </font>
    </PasswordField>
    <TextField fx:id="tbNewLogin" layoutX="105.0" layoutY="113.0" promptText="%newLogin">
        <font>
            <Font name="Century Gothic Italic" size="24.0" />
        </font>
    </TextField>
    <Button id="settings_confirmChangesButton" defaultButton="true" layoutX="140.0" layoutY="304.0" mnemonicParsing="false" onAction="#onBConfirmChanges" text="%confirmButton" textFill="WHITE">
        <font>
            <Font name="Courier New Bold" size="24.0" />
        </font>
    </Button>
</AnchorPane>
